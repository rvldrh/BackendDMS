{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "headers" : {
          "Access-Control-Allow-Origin": "https://dms-bms-frontend.vercel.app/"
        }
      }
    ]
  }
  